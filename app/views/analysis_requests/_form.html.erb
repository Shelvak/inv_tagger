<%= simple_form_for @analysis_request do |f| %>
  <%= f.error_notification %>
  <% @inputs_class = 'span10' %>

  <div class="form-inputs">
    <div class="row-fluid">
      <div class="span6">
        <%= f.label :enrolle_code %>
        <%= enrolle_code_input(f) %>
      </div>
      <div class="span3">
      </div>
      <div class="span2">
        <%= generated_at_picker(f) %>
      </div>
    </div>

    <div class="row-fluid">
      <div class="span6">
        <%= f.label :product_code %>
        <%= product_code_input(f) %>
      </div>

      <div class="span6">
        <%= f.label :variety_code %>
        <%= variety_code_input(f) %>
      </div>
    </div>
  </div>

  <div class="form-actions">
    <div class="pull-right"><%= yield :form_actions %></div>
    <%= f.button :submit, class: 'btn-primary' %>
  </div>
<% end %>
